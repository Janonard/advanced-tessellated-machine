branch routine
move #FF bcc
compare acc bcc
jump_nequal crash

move16 bin acc
move #BEEF bcc
compare acc bcc
jump_nequal crash

halt

crash: dCrash

#include <inclusion routine.atma>
bin:
#binary <inclusion binary.bin>
